#Programming Assignment 3
#Hospital Quality
best <- function(state,outcome)
{
# Read the data File
data <- read.csv("outcome-of-care-measures.csv",colClasses="character")
#state ="TX"
#outcome="heart attack"
# Take out only useful data from there
data <- subset(data,select=c(2,7,11,17,23))
colnames <- c("names","states","heart attack","heart failure","pneumonia")
names(data) <- colnames
# Invalid state and outcome check
if(state %in% unique(data$states)==F) {
stop("invalid state")
} else if(outcome %in% colnames==F) {
stop("invalid outcome")
}
# Select only the state
data <- data[data$states==state,]
#Further reduce the data table to have only the outcome variable
data <- subset(data,data[outcome]!="Not Available")
#Find the minimum value of the outcome
mini <- min(as.numeric(data[[outcome]]))
hospitals <- as.numeric(data[[outcome]]) == mini
#Now we need only the names form the table
hospitals.name <- data[["names"]]
hospitals.name <- hospitals.name[hospitals]
#now order the hospitals.name array
a <- order(hospitals.name)
names(hospitals.name) <-a
hospitals.name[["1"]]
}
best("NY", "pneumonia")
